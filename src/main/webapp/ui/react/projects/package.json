{
  "name": "ps-redux",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "scripts": {
    "start": "npm-run-all --parallel server:start ",
    "start-test": "npm-run-all --parallel test-server cmd-test-karma-conf lint:watch ",
    "debug-karma": "./node_modules/karma/bin/karma  start karma.conf.js --browsers=Chrome",
    "selenium-dev": "node ../../seleniumTestCases/OTTO.js DEV",
    "selenium-stage": "node ../../seleniumTestCases/OTTO.js STAGE",
    "selenium-test": "node ../../seleniumTestCases/OTTO.js TEST",
    "server:start": "babel-node tools/src.server.js",
    "test-server": "babel-node tools/test.server.js",
    "install-node_modules": "npm i",
    "install-opus-server-node_modules": "npm i --unsafe-perm --no-optional",
    "install-opus-server-prod-node_modules": "npm i --unsafe-perm --no-optional --production",
    "build-webpack": "npm run clean-dist && webpack -p --config webpack.config.js*",
    "build-webpack:watch": "npm run build-webpack --watch",
    "build-production": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors --config webpack.config.js",
    "lint": "node_modules/.bin/esw webpack.config* src src/bundle.js tools",
    "lint:watch": "npm run lint -- --watch",
    "lint-prod": "webpack -p --config webpack.config.js",
    "lint-prod:watch": "npm run lint-prod -- --watch",
    "test-karma": "./node_modules/karma/bin/karma start --color --auto-watch karma.conf.js  ",
    "test": "mocha --reporter progress tools/testSetup.js \"src/tests/**/*.test.js\" ",
    "test-karma:watch": "karma start --single-run --browsers PhantomJS \"src/tests/**/*.test.js\" ",
    "test-karma-conf": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch",
    "cmd-test-karma-conf": "node --max_old_space_size=4096 ./node_modules/karma/bin/karma start karma.conf.js --auto-watch",
    "full-test": "NODE_ENV=test karma start karma.conf.js",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "Cory House",
  "license": "MIT",
